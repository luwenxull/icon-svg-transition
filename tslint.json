{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "linebreak-style": [true, "LF"],
        "semicolon": [true, "never"],
        "only-arrow-functions": false,
        "quotemark": [true, "single"],
        "no-empty": false,
        "variable-name": [false],
        "arrow-parens": false,
        "prefer-for-of": false,
        "object-literal-sort-keys": false,
        "indent": [true, "spaces", 2]
    },
    "rulesDirectory": []
}